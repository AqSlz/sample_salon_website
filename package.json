  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "mocha": "^6.2.2",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "typescript": "^3.8.3",
    "bcrypt": "^4.0.1",
    "colors": "^1.4.0",
    "connect-mongo": "^3.2.0",
    "express-session": "^1.17.0",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "validator": "^12.2.0"
  },
